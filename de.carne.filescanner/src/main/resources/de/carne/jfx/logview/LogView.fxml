<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.carne.jfx.logview.LogViewController">
   <children>
      <AnchorPane VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="ctlLogTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <columns>
                <TableColumn fx:id="ctlLogTableLevel" maxWidth="-1.0" minWidth="30.0" prefWidth="-1.0" sortable="false" text="%_STR_LOG_LEVEL" />
                <TableColumn fx:id="ctlLogTableTimestamp" maxWidth="-1.0" minWidth="100.0" prefWidth="-1.0" sortable="false" text="%_STR_LOG_TIMESTAMP" />
                  <TableColumn fx:id="ctlLogTableMessage" maxWidth="1.7976931348623157E308" prefWidth="-1.0" sortable="false" text="%_STR_LOG_MESSAGE" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </AnchorPane>
   </children>
</VBox>
